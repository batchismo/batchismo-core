{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Batchismo",
  "version": "0.1.0",
  "identifier": "dev.batchismo.app",
  "build": {
    "frontendDist": "ui/dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev --prefix bat-shell/ui",
    "beforeBuildCommand": "npm run build --prefix bat-shell/ui"
  },
  "app": {
    "windows": [
      {
        "title": "Batchismo",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "publisher": "Vatch√© Chamlian",
    "icon": ["icons/icon.ico", "icons/icon.png"],
    "resources": {}
  }
}
